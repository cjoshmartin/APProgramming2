
public class Parton {

	private String name;
	private Book book1,book2,book3;
	
	
	public Parton(String n){
		name =n;
	}
	public hasborrow()
	public boolean borrow(Book b)
	{
		if(book1==null)
		{
			book1 = b;
			return true;
		}
		else if (book2==null)
		{
			book2 = b;
			return true;
		}
		else if (book3==null)
		{
			book3 = b;
			return true;
		}
		else{
			return false;
		}
	}
}
